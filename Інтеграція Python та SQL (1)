    "sql_query = \"\"\"\n",
    "SELECT\n",
    "    productName,\n",
    "    productLine,\n",
    "    quantityInStock,\n",
    "    buyPrice\n",
    "FROM\n",
    "    products\n",
    "ORDER BY\n",
    "    quantityInStock DESC\n",
    "LIMIT 10;\n",
