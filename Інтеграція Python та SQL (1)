    "sql_query = text(\"\"\"\n",
    "SELECT\n",
    "    o.orderNumber,\n",
    "    o.orderDate,\n",
    "    o.status,\n",
    "    c.customerName,\n",
    "    c.country,\n",
    "    SUM(od.quantityOrdered * od.priceEach) AS TotalAmount\n",
    "FROM\n",
    "    orders AS o\n",
    "JOIN\n",
    "    customers AS c ON o.customerNumber = c.customerNumber\n",
    "JOIN\n",
    "    orderdetails AS od ON o.orderNumber = od.orderNumber\n",
    "WHERE\n",
    "    YEAR(o.orderDate) = :target_year\n",
    "GROUP BY\n",
    "    o.orderNumber, o.orderDate, o.status, c.customerName, c.country;\n",
