    "sns.lineplot(data=df_dynamics, x='–†—ñ–∫ —Ç–∞ –º—ñ—Å—è—Ü—å –∑–∞–º–æ–≤–ª–µ–Ω—å', y='–ó–∞–≥–∞–ª—å–Ω–∏–π –¥–æ—Ö—ñ–¥', color='blue', label='–î–æ—Ö—ñ–¥')\n",
    "\n",
    "# 2. –î–æ–¥–∞–≤–∞–Ω–Ω—è –ª—ñ–Ω—ñ—ó —Ç—Ä–µ–Ω–¥—É (–ß–µ—Ä–≤–æ–Ω–∞ –ø—É–Ω–∫—Ç–∏—Ä–Ω–∞ –ª—ñ–Ω—ñ—è, —è–∫ –Ω–∞ –∑–æ–±—Ä–∞–∂–µ–Ω–Ω—ñ)\n",
    "\n",
    "# –ü–µ—Ä–µ—Ç–≤–æ—Ä–µ–Ω–Ω—è –¥–∞—Ç –Ω–∞ —á–∏—Å–ª–æ–≤—ñ –∑–Ω–∞—á–µ–Ω–Ω—è –¥–ª—è —Ä–æ–∑—Ä–∞—Ö—É–Ω–∫—É —Ä–µ–≥—Ä–µ—Å—ñ—ó\n",
    "x_values_numeric = pd.to_numeric(df_dynamics['–†—ñ–∫ —Ç–∞ –º—ñ—Å—è—Ü—å –∑–∞–º–æ–≤–ª–µ–Ω—å'])\n",
    "y_values = df_dynamics['–ó–∞–≥–∞–ª—å–Ω–∏–π –¥–æ—Ö—ñ–¥']\n",
    "\n",
    "# –†–æ–∑—Ä–∞—Ö—É–Ω–æ–∫ –∫–æ–µ—Ñ—ñ—Ü—ñ—î–Ω—Ç—ñ–≤ –ª—ñ–Ω—ñ–π–Ω–æ—ó —Ä–µ–≥—Ä–µ—Å—ñ—ó (–Ω–∞—Ö–∏–ª —Ç–∞ –ø–µ—Ä–µ—Ç–∏–Ω)\n",
    "trend_coeffs = np.polyfit(x_values_numeric, y_values, 1)\n",
    "\n",
    "# –°—Ç–≤–æ—Ä–µ–Ω–Ω—è –ª—ñ–Ω—ñ—ó —Ç—Ä–µ–Ω–¥—É –Ω–∞ –æ—Å–Ω–æ–≤—ñ —Ä–æ–∑—Ä–∞—Ö–æ–≤–∞–Ω–∏—Ö –∫–æ–µ—Ñ—ñ—Ü—ñ—î–Ω—Ç—ñ–≤\n",
    "trend_line = np.poly1d(trend_coeffs)(x_values_numeric)\n",
    "\n",
    "# –î–æ–¥–∞–≤–∞–Ω–Ω—è –ª—ñ–Ω—ñ—ó —Ç—Ä–µ–Ω–¥—É –Ω–∞ –≥—Ä–∞—Ñ—ñ–∫ –∑–∞ –¥–æ–ø–æ–º–æ–≥–æ—é plt.plot\n",
    "plt.plot(df_dynamics['–†—ñ–∫ —Ç–∞ –º—ñ—Å—è—Ü—å –∑–∞–º–æ–≤–ª–µ–Ω—å'], trend_line, color='red', linestyle='--', label='–¢—Ä–µ–Ω–¥')\n",
    "\n",
    "\n",
    "# –ù–∞–ª–∞—à—Ç—É–≤–∞–Ω–Ω—è –∑–æ–≤–Ω—ñ—à–Ω—å–æ–≥–æ –≤–∏–≥–ª—è–¥—É –≥—Ä–∞—Ñ—ñ–∫–∞\n",
    "plt.title('–î–æ—Ö—ñ–¥ –ø–æ –º—ñ—Å—è—Ü—è—Ö –∑ —Ç—Ä–µ–Ω–¥–æ–≤–æ—é –ª—ñ–Ω—ñ—î—é')\n",
    "plt.xlabel('–î–∞—Ç–∞')\n",
