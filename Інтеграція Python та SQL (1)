  {
   "cell_type": "code",
   "execution_count": 32,
   "id": "2d3661a4-cce1-409a-8e06-145ebd76c8e1",
   "metadata": {},
   "outputs": [],
   "source": [
    "sql_query = \"\"\"\n",
    "WITH MonthlySales AS (\n",
    "    -- 1. –ê–≥—Ä–µ–≥–∞—Ü—ñ—è –ø—Ä–æ–¥–∞–∂—ñ–≤ —Ç–∞ –∫—ñ–ª—å–∫–æ—Å—Ç—ñ –∑–∞–º–æ–≤–ª–µ–Ω—å –ø–æ —Ä–æ–∫—É —Ç–∞ –º—ñ—Å—è—Ü—é\n",
    "    SELECT\n",
    "        YEAR(o.orderDate) AS SalesYear,\n",
    "        MONTH(o.orderDate) AS SalesMonth,\n",
    "        DATE_FORMAT(o.orderDate, '%Y-%m') AS YearMonth,\n",
    "        COUNT(DISTINCT o.orderNumber) AS OrdersCount,\n",
    "        SUM(od.quantityOrdered * od.priceEach) AS MonthlyRevenue\n",
    "    FROM\n",
    "        orders o\n",
    "    JOIN\n",
    "        orderdetails od ON o.orderNumber = od.orderNumber\n",
    "    GROUP BY\n",
    "        SalesYear, SalesMonth, YearMonth\n",
    "    ORDER BY\n",
    "        SalesYear, SalesMonth\n",
    ")\n",
    "-- 2. –ó–∞—Å—Ç–æ—Å—É–≤–∞–Ω–Ω—è –≤—ñ–∫–æ–Ω–Ω–∏—Ö —Ñ—É–Ω–∫—Ü—ñ–π –¥–ª—è —Ä–æ–∑—Ä–∞—Ö—É–Ω–∫—É –¥–∏–Ω–∞–º—ñ–∫–∏\n",
    "SELECT\n",
    "    YearMonth AS '–†—ñ–∫ —Ç–∞ –º—ñ—Å—è—Ü—å –∑–∞–º–æ–≤–ª–µ–Ω—å',\n",
    "    OrdersCount AS '–ö—ñ–ª—å–∫—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω—å –∑–∞ —Ü–µ–π —Ä—ñ–∫-–º—ñ—Å—è—Ü—å',\n",
    "    MonthlyRevenue AS '–ó–∞–≥–∞–ª—å–Ω–∏–π –¥–æ—Ö—ñ–¥',\n",
    "    -- –ó—Ä–æ—Å—Ç–∞–Ω–Ω—è –ø–æ—Ä—ñ–≤–Ω—è–Ω–æ –∑ –ø–æ–ø–µ—Ä–µ–¥–Ω—ñ–º –º—ñ—Å—è—Ü–µ–º —É %\n",
    "    ROUND(((MonthlyRevenue - LAG(MonthlyRevenue) OVER (ORDER BY SalesYear, SalesMonth)) / LAG(MonthlyRevenue) OVER (ORDER BY SalesYear, SalesMonth)) * 100, 2) AS '–ù–∞ —è–∫–∏–π % –º–∏ –∑—Ä–æ—Å–ª–∏ –ø–æ—Ä—ñ–≤–Ω—è–Ω–æ –∑ –ø–æ–ø–µ—Ä–µ–¥–Ω—ñ–º –º—ñ—Å—è—Ü–µ–º',\n",
    "    -- –ù–∞–∫–æ–ø–∏—á—É–≤–∞–ª—å–Ω–∏–π –¥–æ—Ö—ñ–¥ –∑–∞ —Ä—ñ–∫\n",
    "    SUM(MonthlyRevenue) OVER (PARTITION BY SalesYear ORDER BY SalesMonth) AS '–ù–∞–∫–æ–ø–∏—á—É–≤–∞–ª—å–Ω–∏–π –¥–æ—Ö—ñ–¥ –¥–æ —Ü—å–æ–≥–æ –º—ñ—Å—è—Ü—è –∑–∞ —Ä—ñ–∫',\n",
    "    -- –ö–æ–≤–∑–Ω–µ —Å–µ—Ä–µ–¥–Ω—î –∑–∞ 3 –º—ñ—Å—è—Ü—ñ\n",
    "    AVG(MonthlyRevenue) OVER (ORDER BY SalesYear, SalesMonth ROWS BETWEEN 2 PRECEDING AND CURRENT ROW) AS '–ö–æ–≤–∑–Ω–µ —Å–µ—Ä–µ–¥–Ω—î –¥–æ—Ö–æ–¥—É –∑–∞ 3 –º—ñ—Å—è—Ü—ñ',\n",
    "    -- –†–∞–Ω–≥ –º—ñ—Å—è—Ü—è –∑–∞ –¥–æ—Ö–æ–¥–æ–º\n",
    "    RANK() OVER (ORDER BY MonthlyRevenue DESC) AS '–†–∞–Ω–≥ —Ü—å–æ–≥–æ –º—ñ—Å—è—Ü—è –∑–∞ –¥–æ—Ö–æ–¥–æ–º'\n",
    "FROM\n",
    "    MonthlySales\n",
    "ORDER BY\n",
    "    SalesYear, SalesMonth;\n",
    "\"\"\"\n",
    "df_dynamics = pd.read_sql(text(sql_query), engine)\n",
    "\n",
    "# –ü–µ—Ä–µ—Ç–≤–æ—Ä–µ–Ω–Ω—è —Å—Ç–æ–≤–ø—Ü—è –¥–∞—Ç–∏ –Ω–∞ datetime –æ–±'—î–∫—Ç –¥–ª—è —Ä–æ–±–æ—Ç–∏ –∑ –≥—Ä–∞—Ñ—ñ–∫–∞–º–∏\n",
    "df_dynamics['–†—ñ–∫ —Ç–∞ –º—ñ—Å—è—Ü—å –∑–∞–º–æ–≤–ª–µ–Ω—å'] = pd.to_datetime(df_dynamics['–†—ñ–∫ —Ç–∞ –º—ñ—Å—è—Ü—å –∑–∞–º–æ–≤–ª–µ–Ω—å'])\n",
    "\n",
    "# –û–±—á–∏—Å–ª–µ–Ω–Ω—è —Å–µ—Ä–µ–¥–Ω—å–æ–≥–æ —á–µ–∫—É –¥–ª—è –æ—Å—Ç–∞–Ω–Ω—å–æ–≥–æ –≥—Ä–∞—Ñ—ñ–∫–∞\n",
    "df_dynamics['–°–µ—Ä–µ–¥–Ω—ñ–π —á–µ–∫'] = df_dynamics['–ó–∞–≥–∞–ª—å–Ω–∏–π –¥–æ—Ö—ñ–¥'] / df_dynamics['–ö—ñ–ª—å–∫—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω—å –∑–∞ —Ü–µ–π —Ä—ñ–∫-–º—ñ—Å—è—Ü—å']\n",
    "\n",
    "# –ó–∞–ø–æ–≤–Ω—é—î–º–æ NaN –¥–ª—è –ø–µ—Ä—à–æ–≥–æ –º—ñ—Å—è—Ü—è, —â–æ–± –≥—Ä–∞—Ñ—ñ–∫–∏ –Ω–µ –º–∞–ª–∏ –ø—Ä–æ–±—ñ–ª—ñ–≤\n",
    "df_dynamics.fillna(0, inplace=True)\n"
   ]
