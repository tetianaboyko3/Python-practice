  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "d4c514ce-479f-4050-b5db-f212ba509b19",
   "metadata": {},
   "outputs": [],
   "source": [
    "sql_query = \"\"\"\n",
    "WITH ProductRevenue AS (\n",
    "    -- 1. –û–±—á–∏—Å–ª–µ–Ω–Ω—è –∑–∞–≥–∞–ª—å–Ω–æ–≥–æ –¥–æ—Ö–æ–¥—É –¥–ª—è –∫–æ–∂–Ω–æ–≥–æ –ø—Ä–æ–¥—É–∫—Ç—É\n",
    "    SELECT\n",
    "        p.productCode,\n",
    "        p.productName,\n",
    "        p.productLine,\n",
    "        SUM(od.quantityOrdered * od.priceEach) AS TotalProductRevenue\n",
    "    FROM\n",
    "        products p\n",
    "    JOIN\n",
    "        orderdetails od ON p.productCode = od.productCode\n",
    "    GROUP BY\n",
    "        p.productCode, p.productName, p.productLine\n",
    "),\n",
    "TotalCompanyRevenue AS (\n",
    "    -- 2. –û–±—á–∏—Å–ª–µ–Ω–Ω—è –∑–∞–≥–∞–ª—å–Ω–æ–≥–æ –¥–æ—Ö–æ–¥—É –≤—Å—ñ—î—ó –∫–æ–º–ø–∞–Ω—ñ—ó (–≤–∏–∫–æ—Ä–∏—Å—Ç–æ–≤—É—î–º–æ –≤—ñ–∫–æ–Ω–Ω—É —Ñ—É–Ω–∫—Ü—ñ—é SUM OVER)\n",
    "    SELECT\n",
    "        SUM(TotalProductRevenue) OVER () AS GrandTotal\n",
    "    FROM\n",
    "        ProductRevenue\n",
    "    LIMIT 1\n",
    "),\n",
    "LineAverageRevenue AS (\n",
    "    -- 3. –û–±—á–∏—Å–ª–µ–Ω–Ω—è —Å–µ—Ä–µ–¥–Ω—å–æ–≥–æ –¥–æ—Ö–æ–¥—É –ø–æ –∫–æ–∂–Ω—ñ–π –ª—ñ–Ω—ñ–π—Ü—ñ –ø—Ä–æ–¥—É–∫—Ç—ñ–≤\n",
    "    SELECT\n",
    "        productLine,\n",
    "        AVG(TotalProductRevenue) AS AvgLineRev\n",
    "    FROM\n",
    "        ProductRevenue\n",
    "    GROUP BY\n",
    "        productLine\n",
    ")\n",
    "-- 4. –§—ñ–Ω–∞–ª—å–Ω–∏–π –≤–∏–±—ñ—Ä –¥–∞–Ω–∏—Ö –∑ –≤–∏–∫–æ—Ä–∏—Å—Ç–∞–Ω–Ω—è–º –≤—ñ–∫–æ–Ω–Ω–∏—Ö —Ñ—É–Ω–∫—Ü—ñ–π —Ç–∞ –æ–±'—î–¥–Ω–∞–Ω—å\n",
    "SELECT\n",
    "    pr.productName AS '–ù–∞–∑–≤–∞ –ø—Ä–æ–¥—É–∫—Ç—É',\n",
    "    pr.productLine AS '–õ—ñ–Ω—ñ–π–∫–∞ –ø—Ä–æ–¥—É–∫—Ç—É',\n",
    "    pr.TotalProductRevenue AS '–ó–∞–≥–∞–ª—å–Ω–∏–π –¥–æ—Ö—ñ–¥ –∑ –ø—Ä–æ–¥—É–∫—Ç—É',\n",
    "    RANK() OVER (ORDER BY pr.TotalProductRevenue DESC) AS '–†–∞–Ω–≥ –ø—Ä–æ–¥—É–∫—Ç—É –ø–æ –¥–æ—Ö–æ–¥—É',\n",
    "    (pr.TotalProductRevenue / tcr.GrandTotal) * 100 AS '–í—ñ–¥—Å–æ—Ç–æ–∫ –≤—ñ–¥ –∑–∞–≥–∞–ª—å–Ω–æ–≥–æ –¥–æ—Ö–æ–¥—É –∫–æ–º–ø–∞–Ω—ñ—ó',\n",
    "    ((pr.TotalProductRevenue - lar.AvgLineRev) / lar.AvgLineRev) * 100 AS '–†—ñ–∑–Ω–∏—Ü—è –∑ —Å–µ—Ä–µ–¥–Ω—ñ–º –¥–æ—Ö–æ–¥–æ–º –ø–æ –ª—ñ–Ω—ñ–π—Ü—ñ –ø—Ä–æ–¥—É–∫—Ç—É (%)'\n",
    "FROM\n",
    "    ProductRevenue pr\n",
    "JOIN\n",
    "    TotalCompanyRevenue tcr ON 1=1\n",
    "JOIN\n",
    "    LineAverageRevenue lar ON pr.productLine = lar.productLine\n",
    "ORDER BY\n",
    "    '–í—ñ–¥—Å–æ—Ç–æ–∫ –≤—ñ–¥ –∑–∞–≥–∞–ª—å–Ω–æ–≥–æ –¥–æ—Ö–æ–¥—É –∫–æ–º–ø–∞–Ω—ñ—ó' DESC;\n",
    "\"\"\" \n",
    "\n",
    "# –í–∏–∫–æ–Ω–∞–Ω–Ω—è SQL-–∑–∞–ø–∏—Ç—É —Ç–∞ –∑–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤ —É DataFrame\n",
    "df_profit = pd.read_sql(text(sql_query), engine)\n",
    "\n"
   ]
